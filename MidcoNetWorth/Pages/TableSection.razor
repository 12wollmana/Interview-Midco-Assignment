@using Data;

<tr>
    @GetSectionTitle()
</tr>


@code {
    [Parameter]
    public TableSectionData? sectionData { get; set; }

    public String GetSectionTitle() {
        if(sectionData == null) {
            return String.Empty;
        }
        else
        {
                return sectionData.title;
        }
    }

}

